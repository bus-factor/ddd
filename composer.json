{
  "name": "bus-factor/ddd",
  "description": "Domain Driven Design related Classes and Utilities",
  "type": "library",
  "archive": {
    "exclude": [
      "/.gitattributes",
      "/.gitignore",
      "/.travis.yml",
      "/phpunit.xml",
      "/tests"
    ]
  },
  "keywords": [
    "ddd",
    "domain-driven design",
    "domain driven design"
  ],
  "support": {
    "issues": "https://github.com/bus-factor/ddd/issues"
  },
  "readme": "README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael LeÃŸnau",
      "email": "michael.lessnau@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "BusFactor\\Ddd\\": "src/BusFactor/Ddd"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\BusFactor\\Ddd\\": "tests/BusFactor/Ddd"
    }
  },
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ramsey/uuid": ">=4.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7"
  },
  "scripts": {
    "test": "phpunit --no-coverage",
    "test:cov:text": "phpunit --coverage-text=php://stdout",
    "test:cov:html": "phpunit --coverage-html=coverage"
  }
}
